    <DataSet Name="dsKPI">
      <Query>
        <DataSourceName>localhost</DataSourceName>
        <CommandText>SELECT * 
FROM (
SELECT counter_name,cntr_value
FROM sys.dm_os_performance_counters
WHERE 1=1
and cntr_type = 65792 
--and cntr_value &gt; 0
--and instance_name in ('_Total')
and counter_name in ('User Connections','Active Transactions','Processes blocked')) a
PIVOT (max(cntr_value) FOR counter_name in 
([User Connections],[Active Transactions],[Processes blocked]) ) b</CommandText>
      </Query>
      <Fields>
        <Field Name="User_Connections">
          <DataField>User Connections</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Active_Transactions">
          <DataField>Active Transactions</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Processes_blocked">
          <DataField>Processes blocked</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
